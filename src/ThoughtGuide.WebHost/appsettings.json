{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "App": {
    "ApiKey": ""
  },
  "EfCore": {
    "ApplyMigrationsAutomatically": false,
    "EnableSensitiveDataLogging": false
  },
  "LoggingFeatures": {
    "HttpLogging": {
      "RequestHeaders": false,
      "RequestProtocol": true,
      "RequestScheme": true,
      "RequestMethod": true,
      "RequestPath": true,
      "RequestQuery": true,
      "RequestBody": false,
      "ResponseHeaders": true,
      "ResponseStatusCode": true,
      "ResponseBody": false
    },
    "NLog": {
      "ConfigFile": "NLog.config",
      "MinLevels": {
        "StartupLogs": "Debug",
        "AppLogs": "Debug",
        "RequestLogs": "Info",
        "EfCoreLogs": "Warn",
        "OtherLogs": "Warn"
      }
    },
    "Loki": {
      "Endpoint": "http://localhost:3100"
    }
  },
  "AllowedHosts": "*"
}